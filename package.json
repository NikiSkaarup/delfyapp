{
  "name": "delfyapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "mobx": "^3.2.2",
    "mobx-react": "^4.2.2",
    "node-sass-chokidar": "^0.0.3",
    "react": "^15.6.1",
    "react-app-rewire-mobx": "^1.0.6",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.13"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "mobx-react-devtools": "^4.2.15",
    "react-app-rewired": "^1.2.5"
  },
  "scripts": {
    "build-css": "node-sass-chokidar ./src/styles -o ./src/css",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/styles -o ./src/css --watch --recursive",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom"
  },
  "repository": {
    "url": "https://github.com/nikiskaarup/delfyapp"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "plugins": [
      "transform-decorators-legacy"
    ],
    "presets": [
      "react-app"
    ]
  }
}
